@using Newtonsoft.Json.Linq
@model JObject

@{
    var value = Model["value"]?.ToString();
    var marks = Model["marks"] as JArray;

    var isBold = marks?.Any(mark => mark["type"]?.ToString() == "bold") ?? false;

    if (isBold)
    {
        <strong>@value</strong>
    }
    else
    {
        @value
    }
}