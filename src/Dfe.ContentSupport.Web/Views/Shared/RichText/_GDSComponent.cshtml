@model ContentItemBase

@{
    var contentTypeId = Model.Data.Target.Sys.ContentType?.Sys.Id;
    switch (contentTypeId)
    {
        case "Attachment":
            <partial name="RichText/Custom/_Attachment" model="Model" />
            break;
        case "csCard":
            <partial name="RichText/Custom/_Card" model="Model.Data.Target" />
            break; 
        case "GridContainer":
            <partial name="RichText/Custom/_GridContainer" model="Model" />
            break;
        default:
            break;
    }
}