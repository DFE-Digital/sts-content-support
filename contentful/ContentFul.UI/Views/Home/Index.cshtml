@using Contentful.Core.Models
@model List<CreditCard>


@{
    ViewData["Title"] = "Home Page";
}

@foreach(var card in Model){
    <h2>Issue :@card.Issuer</h2>
     <h2>Type : @card.Type</h2>
      <h2>Fee : @card.Fee</h2>
}

<div class="text-center">
    <h1 class="display-4">Welcome</h1>
